{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "publicfiles" }
    }
  ],
  "routes": [
    { "src": "/src/(.*)", "status": 404 },
    { "src": "/publicfiles/src/(.*)", "status": 404 },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
